{
    "name": "Automatic Detection Approach",
    "fields": [
        {
            "name": "ID",
            "type": "identifier",
            "format": "AD^0\\d{3}$",
            "unique": true,
            "mandatory": true
        }, {
            "name": "Name",
            "type": "string",
            "unique": true,
            "mandatory": true
        }, {
            "name": "Shortcut",
            "type": "string"
        }, {
            "name": "Detected Descriptions",
            "type": "reflist",
            "elements": "description",
            "mandatory": true
        }, {
            "name": "Type",
            "type": "enum",
            "values": ["Rule-based", "Supervised ML", "Unsupervised ML", "Supervised DL", "Unsupervised DL"],
            "mandatory": true
        }, {
            "name": "Accessibility",
            "type": "enum",
            "values": ["Open Access Link", "Open Source", "Reachable Link", "Broken Link", "No Link", "Upon Request", "Private", "Proprietary"],
            "mandatory": true
        }, {
            "name": "Source Link",
            "type": "string"
        }, {
            "name": "Empirical Method applied",
            "type": "boolean",
            "mandatory": true
        }, {
            "name": "Practitioners involved",
            "type": "boolean",
            "mandatory": true
        }, {
            "name": "Releases",
            "type": "cluster",
            "dimensions": ["Tool", "Webservice", "Library", "API", "Source Code", "Notebook", "Pre-Trained Model"],
            "values": ["y", "?", "n"]
        }, {
            "name": "Necessary Information",
            "type": "cluster",
            "dimensions": ["POS tags", "Dependency tags", "Consistyency tags", "Lemmatization", "Stemming", "Phrase Chunks", "Stop word removal", "Semantic role labeling", "Thesaurus", "Named Entity Recognition", "Parse Tree"],
            "values": ["y", "?", "n"]
        }
    ]
}